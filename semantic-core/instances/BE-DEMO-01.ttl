# ============================================================
# BE-DEMO-01 — Demo Belgian apartment (ABox)
#
# A Brussels apartment with compact single-floor layout,
# gas condensing boiler, and Belgian electrical norms.
# French-language naming (Brussels context).
# ============================================================

# ── The property itself ──

home:BE-DEMO-01
  a dhc:RealEstate ;
  dhc:smartHomeId "BE-DEMO-01" ;
  dhc:name "Demo Appartement Belgique" ;
  dhc:description "Un appartement de démonstration bruxellois pour la plateforme DigitalHome.Cloud." ;
  dhc:hasArea home:Appartement ;
  dhc:hasHVACSystem home:ChauffageGaz ;
  dhc:hasPlumbingSystem home:EauVille ;
  dhc:hasNetworkSegment home:ReseauLocal ;
  dhc:hasElectricVehicle home:TrottinetteElectrique ;
  dhc:hasChargingStation home:PriseGarage .

# ── Spatial: Areas & Floors ──

home:Appartement
  a dhc:Area ;
  dhc:name "Appartement" ;
  dhc:hasFloor home:NiveauUnique .

home:NiveauUnique
  a dhc:Floor ;
  dhc:name "Niveau unique" ;
  dhc:floorLevel 2 ;
  dhc:hasSpace home:Sejour ;
  dhc:hasSpace home:Cuisine ;
  dhc:hasSpace home:Chambre ;
  dhc:hasSpace home:SalleDeBains .

# ── Spatial: Spaces ──

home:Sejour
  a dhc:Space ;
  dhc:name "Séjour" ;
  dhc:areaM2 24.0 ;
  dhc:hasEquipment home:SEJ_Lumiere1 ;
  dhc:hasEquipment home:SEJ_Prise1 ;
  dhc:hasEquipment home:SEJ_Thermostat .

home:Cuisine
  a dhc:Space ;
  dhc:name "Cuisine" ;
  dhc:areaM2 10.0 ;
  dhc:hasEquipment home:CUI_Lumiere1 ;
  dhc:hasEquipment home:CUI_Prise1 ;
  dhc:hasFixture home:CUI_Evier .

home:Chambre
  a dhc:Space ;
  dhc:name "Chambre" ;
  dhc:areaM2 14.0 ;
  dhc:hasEquipment home:CH_Lumiere1 ;
  dhc:hasEquipment home:CH_Thermostat .

home:SalleDeBains
  a dhc:Space ;
  dhc:name "Salle de bains" ;
  dhc:areaM2 6.0 ;
  dhc:hasFixture home:SDB_Douche ;
  dhc:hasFixture home:SDB_WC ;
  dhc:hasFixture home:SDB_Lavabo .

# ── Electrical: Distribution & Circuits ──

home:CoffrePrincipal
  a dhc:DistributionBoard ;
  dhc:name "Coffret principal" ;
