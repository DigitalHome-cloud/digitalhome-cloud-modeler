  dhc:hasCircuit home:CircuitPrises ;
  dhc:hasCircuit home:CircuitCuisine .

home:CircuitEclairage
  a dhc:Circuit ;
  dhc:name "Éclairage" ;
  dhc:maxPoints 8 ;
  dhc:hasProtection home:Diff_30mA ;
  dhc:feedsEquipment home:SEJ_Lumiere1 ;
  dhc:feedsEquipment home:CUI_Lumiere1 .

home:CircuitPrises
  a dhc:Circuit ;
  dhc:name "Prises" ;
  dhc:maxPoints 8 ;
  dhc:hasProtection home:Disj_Prises ;
